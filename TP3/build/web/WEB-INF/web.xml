<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">

    <display-name>Servlets</display-name>
    <description> Servlets </description>- 

    <!-- 
     Define servlets that are included in the example application 
    --> 
    - <servlet>
        <servlet-name>HelloWorldServlet</servlet-name>
        <servlet-class>HelloWorldServlet</servlet-class>
    </servlet>


    - <servlet>
        <servlet-name>HelloToServlet</servlet-name>
        <servlet-class>HelloToServlet</servlet-class>
    </servlet>

    - <servlet>
        <servlet-name>DateServlet</servlet-name>
        <servlet-class>DateServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>QuidServlet</servlet-name>
        <servlet-class>QuidServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HelloWorldServlet</servlet-name>
        <url-pattern>/servlet/HelloWorldServlet</url-pattern>
    </servlet-mapping>

    - <servlet-mapping>
        <servlet-name>HelloToServlet</servlet-name>
        <url-pattern>/servlet/HelloToServlet</url-pattern>
    </servlet-mapping>


    <servlet-mapping>
        <servlet-name>DateServlet</servlet-name>
        <url-pattern>/servlet/DateServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>QuidServlet</servlet-name>
        <url-pattern>/servlet/QuidServlet</url-pattern>
    </servlet-mapping>
</web-app>